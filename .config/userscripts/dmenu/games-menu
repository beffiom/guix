#!/bin/sh
case "$(printf "retroarch\\nopenrct2\\nparkitect\\nminetest\\nxonotic\\nrunescape\\nppsspp\\npcsx2" | dmenu)" in
	"retroarch")  retroarch ;;
	"openrct2")  openrct2 ;;
	"parkitect") sh ~/GOG\ Games/Parkitect/start.sh ;;
	"minetest") minetest ;;
	"xonotic")  xonotic-sdl ;;
	"runescape") runelite ;;
	"ppsspp") PPSSPPSDL ;;
	"pcsx2")  PCSX2 ;;
esac
